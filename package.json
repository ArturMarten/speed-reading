    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "husky": "^5.0.9",
    "immutable": "^3.8.2",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
