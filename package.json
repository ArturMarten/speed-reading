  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "docker-start": "npm run docker-build && npm run docker-run",
    "docker-build": "docker build -t speed-reading .",
    "docker-run": "docker run speed-reading",
    "build": "react-scripts build --profile && cp build/service-worker.js build/sw.js",
    "build-patch": "npm version patch --no-commit-hooks --no-git-tag-version && node ./replace.build.js && npm run build",
    "analyze": "source-map-explorer build/static/js/main.*",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "prettier": "prettier \"**/*.+(js|jsx|json|css)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-format lint test:no-watch build",
    "test": "react-scripts test",
    "test:watch": "npm run test -- --watchAll=true",
    "test:no-watch": "npm run test -- --watchAll=false",
    "test:debug": "react-scripts --inspect-brk test --runInBand",
    "test:coverage": "npm run test:no-watch -- --coverage",
    "serve": "serve -s build",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "postinstall": "husky install"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!/node_modules/",
      "!src/index.js",
      "!src/serviceWorkerRegistration.js",
      "!src/service-worker.js",
      "!src/reportWebVitals.js",
      "!src/polyfill/*",
      "!src/stories/*",
      "!src/utils/logger.js",
      "!src/test/**/*"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 10",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
