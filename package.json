  "scripts": {
    "start": "SET BROWSER=none&react-scripts start",
    "build": "react-scripts build",
    "build-patch": "npm version patch --no-commit-hooks --no-git-tag-version && node ./replace.build.js && react-scripts build",
    "analyze": "source-map-explorer build/static/js/main.*",
    "lint": "eslint src/ || exit 0",
    "lint-fix": "eslint src/ || exit 0",
    "test": "SET NODE_ENV=test& mocha",
    "test-watch": "SET NODE_ENV=test& mocha --reporter list --watch --watch-extensions jsx",
    "test-debug": "SET NODE_ENV=test& mocha --inspect-brk",
    "test-coverage": "SET NODE_ENV=test& nyc --extension .jsx mocha",
    "test-jest": "react-scripts test --runInBand --env=jsdom",
    "test-jest-debug": "react-scripts --inspect-brk test --runInBand --env=jsdom",
    "test-jest-coverage": "react-scripts test --coverage --env=jsdom",
    "serve": "serve -s build",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  }
