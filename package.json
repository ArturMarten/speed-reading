  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "docker-start": "npm run docker-build && npm run docker-run",
    "docker-build": "docker build -t speed-reading .",
    "docker-run": "docker run speed-reading",
    "build": "react-scripts build",
    "build-patch": "npm version patch --no-commit-hooks --no-git-tag-version && node ./replace.build.js && react-scripts build",
    "analyze": "source-map-explorer build/static/js/main.*",
    "lint": "eslint src/ || exit 0",
    "lint-fix": "eslint src/ || exit 0",
    "test": "cross-env NODE_ENV=test mocha",
    "test-watch": "cross-env NODE_ENV=test mocha --reporter list --watch --watch-extensions jsx",
    "test-debug": "cross-env NODE_ENV=test mocha --inspect-brk",
    "test-coverage": "cross-env NODE_ENV=test nyc mocha",
    "test-jest": "react-scripts test --runInBand --env=jsdom",
    "test-jest-debug": "react-scripts --inspect-brk test --runInBand --env=jsdom",
    "test-jest-coverage": "react-scripts test --coverage --env=jsdom",
    "serve": "serve -s build",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ]
